   "source": [
    "def Fibo(num):\n",
    "    if num == 0:\n",
    "        return 0\n",
    "    elif num == 1:\n",
    "        return 1\n",
    "    elif num >= 2:\n",
    "        return Fibo(num-1) + Fibo(num-2)"
   ]
