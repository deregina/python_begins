   "source": [
    "# 시간초과"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "def devide(num, num_by) :\n",
    "    return [num//num_by, num%num_by]    "
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "num_by = int(input())"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "num = 0\n",
    "result_list = []\n",
    "\n",
    "while num <= num_by**2 :\n",
    "    try_list = devide(num, num_by)\n",
    "    if try_list[0] == try_list[1] :\n",
    "        result_list.append(num)\n",
    "    num += 1"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "result = 0\n",
    "\n",
    "for i in range(len(result_list)) :\n",
    "    result += result_list[i]\n",
    "    \n",
    "print(result)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
